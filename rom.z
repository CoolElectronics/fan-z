x = 0
y = 0
s = 0
draw {
   clear color 0 255 0
   if key "Right"{
      x = x + 1
   }
   if key "Left"{
      x = x - 1
   }
   if key "Up"{
      y = y - 1
   }
   if key "Down"{
      y = y + 1
   }
   if key "MouseLeft"{
      s = s + 1
   }
   if key "MouseRight"{
      s = s - 1
   }
   rect x y x + s y + s color 255 255 255
}